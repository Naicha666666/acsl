#include <iostream>
#include<algorithm>
using namespace std;
int main() {
    freopen("promote.in", "r", stdin);
    freopen("promote.out", "w", stdout);
    int b1, b2, s1, s2, g1, g2, p1, p2, a, b, c;
    cin >> b1 >> b2 >> s1 >> s2 >> g1 >> g2 >> p1 >> p2;
    a = p2 - p1;
    b = g2 - g1 + p2 - p1;
    c = s2 - s1 + g2 - g1 + p2 - p1;
    cout << c << "\n" << b << "\n" << a << endl;
    return 0;
}
