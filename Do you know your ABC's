#include <iostream>
#include<algorithm>
using namespace std;
int arr[7];
//void setIO(string s) {
//    freopen((s + ".in").c_str(), "r", stdin);
//    freopen((s + ".out").c_str(), "w", stdout);
//}
int main() {
    for(int i = 0;i<7;i++){
        cin>>arr[i];
    }
    sort(arr, arr + 7);
    int a = arr[0];
    int b = arr[1];
    int c = arr[6] - a - b;
    cout << a << " " << b << " " << c;
    return 0;
}
